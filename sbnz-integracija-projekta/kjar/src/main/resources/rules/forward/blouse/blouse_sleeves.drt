template header
sleeveMin
sleeveMax
correction
note

template "Blouse Sleeve Correction"
rule "Blouse Sleeve Correction @{row.rowNumber}"
when
    $blouse : Blouse( sleeveLength >= @{sleeveMin}, sleeveLength < @{sleeveMax} )
then
    modify($blouse) { addCorrection(@{correction}); }
    System.out.println("Blouse sleeve correction: @{note}");
end
end template
