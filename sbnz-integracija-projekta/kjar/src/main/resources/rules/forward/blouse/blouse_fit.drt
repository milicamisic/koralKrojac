template header
fitType
stretch
correction
note

template "Blouse Fit Correction"
rule "Blouse Fit Correction @{row.rowNumber}"
when
    $blouse : Blouse( fit == "@{fitType}", stretchPercent >= @{stretch} )
then
    modify($blouse) { addCorrection(@{correction}); }
    System.out.println("Blouse fit correction: @{note}");
end
end template
