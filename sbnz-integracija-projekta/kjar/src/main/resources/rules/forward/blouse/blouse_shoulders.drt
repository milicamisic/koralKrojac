template header
shoulderMin
shoulderMax
correction
note

template "Blouse Shoulder Correction"
rule "Blouse Shoulder Correction @{row.rowNumber}"
when
    $blouse : Blouse( shoulders >= @{shoulderMin}, shoulders < @{shoulderMax} )
then
    modify($blouse) { addCorrection(@{correction}); }
    System.out.println("Blouse shoulder correction: @{note}");
end
end template
