template header
collarType
correction
note

template "Blouse Collar Correction"
rule "Blouse Collar Correction @{row.rowNumber}"
when
    $blouse : Blouse( collar == "@{collarType}" )
then
    modify($blouse) { addCorrection(@{correction}); }
    System.out.println("Blouse collar correction: @{note}");
end
end template
